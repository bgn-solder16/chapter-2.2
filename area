#include<stdio.h>
int main()
{
    double a,b,c;
    scanf("%lf%lf%lf",&a,&b,&c);
    double rt=(1.0/2)*a*c;
    double cir=3.14159*c*c;
    double trap=(1.0/2)*(a+b)*c;
    double spr=b*b;
    double rac=a*b;
    printf("TRIANGULO: %.3lf\n",rt);
    printf("CIRCULO: %.3lf\n",cir);
    printf("TRAPEZIO: %.3lf\n",trap);
    printf("QUADRADO: %.3lf\n",spr);
    printf("RETANGULO: %.3lf\n",rac);
    return 0;
}
